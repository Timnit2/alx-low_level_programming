/**
 * print_sign - checkinkg no
 * @n: the int to check no
 * Return: 1 if n is greater than o, 0 if n is 0, -1 if n is less than 0
 */
int print_sign(int n)
{
        if (n > o)
        {
                _putchar('+');
                return (1);
        }
        else if (n == 0)
        {
                _putchar(48);
                return (0);
        }
        else if (n < 0)
        {
                _putchar('-');
